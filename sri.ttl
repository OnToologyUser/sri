###  http://purl.org/linked-data/sdmx/2009/measure#obsValue
sdmx-measure:obsValue rdf:type owl:DatatypeProperty .


###  https://eu-phoenix.eu/sri#icscore
:icscore rdf:type owl:DatatypeProperty ;
         rdfs:range [ rdf:type rdfs:Datatype ;
                      owl:unionOf ( xsd:decimal
                                    xsd:double
                                    xsd:float
                                  )
                    ] ;
         rdfs:label "impact criterion score"@en .


###  https://eu-phoenix.eu/sri#score
:score rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf sdmx-measure:obsValue ;
       rdf:type owl:FunctionalProperty ;
       rdfs:range xsd:integer .


###  https://eu-phoenix.eu/sri#share
:share rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:range [ rdf:type rdfs:Datatype ;
                    owl:unionOf ( xsd:decimal
                                  xsd:double
                                  xsd:float
                                )
                  ] ;
       rdfs:label "share"@en .


###  https://eu-phoenix.eu/sri#weight
:weight rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf sdmx-measure:obsValue ;
        rdf:type owl:FunctionalProperty ;
        rdfs:range [ rdf:type rdfs:Datatype ;
                     owl:unionOf ( xsd:decimal
                                   xsd:double
                                   xsd:float
                                 )
                   ] .


